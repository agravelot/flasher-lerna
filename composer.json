{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.2.0",
    "ext-gd": "*",
    "ext-imagick": "*",
    "ext-json": "*",
    "anhskohbo/no-captcha": "^3.0",
    "beyondcode/laravel-self-diagnosis": "^1.0",
    "cviebrock/eloquent-sluggable": "^4.8",
    "doctrine/dbal": "^2.9",
    "fideloper/proxy": "^4.0",
    "intervention/imagecache": "^2.3",
    "ipunkt/laravel-analytics": "^1.5",
    "lab404/laravel-impersonate": "^1.2",
    "laravel/framework": "5.8.*",
    "laravel/horizon": "^3.0",
    "laravel/passport": "^7.2",
    "laravel/slack-notification-channel": "^2.0",
    "laravel/tinker": "^1.0",
    "nwidart/laravel-modules": "^5.0",
    "pion/laravel-chunk-upload": "^1.2",
    "sentry/sentry-laravel": "^1.0",
    "spatie/laravel-activitylog": "^3.1",
    "spatie/laravel-backup": "^6.1",
    "spatie/laravel-medialibrary": "^7.5",
    "spatie/laravel-query-builder": "^1.13",
    "spatie/laravel-sitemap": "^5.3"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.5",
    "beyondcode/laravel-dump-server": "^1.2",
    "beyondcode/laravel-query-detector": "^1.0",
    "beyondcode/laravel-tinker-server": "^1.0",
    "filp/whoops": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.13",
    "fzaninotto/faker": "^1.8",
    "infection/infection": "^0.12.0",
    "joedixon/laravel-translation": "^0.1.3",
    "lanin/laravel-api-debugger": "^3.4",
    "laravel/telescope": "^2.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "nunomaduro/larastan": "^0.3.16",
    "nunomaduro/phpinsights": "^1.2",
    "pheromone/phpcs-security-audit": "^2.0",
    "phploc/phploc": "*",
    "phpmd/phpmd": "^2.6",
    "phpmetrics/phpmetrics": "^2.4",
    "phpunit/phpunit": "^8.0",
    "sllh/composer-versions-check": "^2.0",
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "modules/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan ide-helper:models -W",
      "vendor/bin/php-cs-fixer fix"
    ],
    "format": [
      "vendor/bin/php-cs-fixer fix"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
