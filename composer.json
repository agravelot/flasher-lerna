{
  "name": "nevax/flasher",
  "description": "CMS solution for photographer.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.4.0",
    "ext-gd": "*",
    "ext-imagick": "*",
    "ext-json": "*",
    "ext-pdo_pgsql": "*",
    "ext-redis": "*",
    "algolia/algoliasearch-client-php": "^2.2",
    "anhskohbo/no-captcha": "^3.1",
    "bensampo/laravel-enum": "^1.20",
    "beyondcode/laravel-self-diagnosis": "^1.0",
    "cviebrock/eloquent-sluggable": "^7.0",
    "doctrine/dbal": "^2.9",
    "dreamonkey/laravel-cloudfront-url-signer": "^3.0",
    "fideloper/proxy": "^4.0",
    "fruitcake/laravel-cors": "^2.0",
    "intervention/imagecache": "^2.3",
    "ipunkt/laravel-analytics": "^3.0",
    "jerodev/laravel-font-awesome": "3.0.x-dev",
    "lab404/laravel-impersonate": "^1.2",
    "laravel-validation-rules/colour": "^1.2",
    "laravel/framework": "^7.0",
    "laravel/horizon": "^4.0",
    "laravel/scout": "^8.0",
    "laravel/slack-notification-channel": "^2.0",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^2.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "league/flysystem-cached-adapter": "^1.0",
    "robsontenorio/laravel-keycloak-guard": "^1.3",
    "sentry/sentry-laravel": "^1.0",
    "spatie/laravel-feed": "^2.4",
    "spatie/laravel-medialibrary": "^8.0",
    "spatie/laravel-query-builder": "^2.0.1",
    "spatie/laravel-responsecache": "^6.0",
    "spatie/laravel-sitemap": "^5.3",
    "spatie/schema-org": "^2.9",
    "vizir/laravel-keycloak-web-guard": "^1.5"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.6",
    "beyondcode/laravel-dump-server": "^1.4",
    "beyondcode/laravel-query-detector": "^1.0",
    "facade/ignition": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.13",
    "fruitcake/laravel-telescope-toolbar": "^1.1",
    "fzaninotto/faker": "^1.8",
    "lanin/laravel-api-debugger": "^3.4",
    "laravel/telescope": "^3.2",
    "matt-allan/laravel-code-style": "^0.5.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^4.0",
    "nunomaduro/larastan": "^0.5.0",
    "pheromone/phpcs-security-audit": "^2.0",
    "phpmetrics/phpmetrics": "^2.4",
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-master",
    "sllh/composer-versions-check": "^2.0"
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@composer format",
      "@composer ide-helper"
    ],
    "format": [
      "vendor/bin/php-cs-fixer fix"
    ],
    "ide-helper": [
        "@php artisan ide-helper:generate",
        "@php artisan ide-helper:meta",
        "@php artisan ide-helper:models -N"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
